{
  "integrations": [
    {
      "name": "plaid",
      "type": "import",
      "settings": {
        "environment": "development",
        "api_key1": "",
        "api_key2": ""
      }
    },
    {
      "name": "google",
      "type": "export",
      "settings": {
        "oauth_url": "http://localhost:3000/google-sheets-oauth2callback",
        "template": {
          "document_id": "",
          "sheet_id": ""
        },
        "api_key1": "",
        "api_key2": ""
      }
    }
  ],
  "accounts": [
    {
      "name": "Chase",
      "provider": "plaid",
      "api_key": ""
    }
  ],
  "balances": {
    "enabled": true,
    "columns": [
      {
        "path": "name",
        "name": "Bank Name",
        "type": "automated"
      },
      {
        "path": "official_name",
        "name": "Account Name",
        "type": "automated"
      },
      {
        "path": "type",
        "name": "Type",
        "type": "automated"
      },
      {
        "path": "balances.available",
        "name": "Available Balance",
        "type": "automated"
      },
      {
        "path": "balances.current",
        "name": "Current Balance",
        "type": "automated"
      },
      {
        "path": "balances.limit",
        "name": "Balance Limit",
        "type": "automated"
      }
    ]
  },
  "transactions": {
    "enabled": true,
    "columns": [
      {
        "path": "date",
        "name": "Date",
        "type": "automated"
      },
      {
        "path": "amount",
        "name": "Amount",
        "type": "automated"
      },
      {
        "path": "name",
        "name": "Title",
        "type": "automated"
      },
      {
        "path": "account",
        "name": "Account",
        "type": "automated"
      },
      {
        "path": "category.0",
        "name": "Category",
        "type": "automated",
        "overrides": [
          {
            "match": "Wegman's|Publix|Safeway",
            "replace": "Grocery Stores"
          }
        ]
      },
      {
        "path": "category.1",
        "name": "Subcategory",
        "type": "automated"
      },
      {
        "path": "pending",
        "name": "Pending?",
        "type": "automated"
      },
      {
        "path": "work",
        "name": "Work Expense (Y/N)?",
        "type": "manual"
      },
      {
        "path": "joint",
        "name": "Joint Expense (Y/N)?",
        "type": "manual"
      },
      {
        "path": "notes",
        "name": "Notes",
        "type": "manual"
      }
    ]
  }
}
